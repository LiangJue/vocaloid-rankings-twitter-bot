{
  "name": "miku_twitter_bot",
  "version": "1.0.0",
  "description": "This is a twitter bot that updates about hatsune miku related info periodically",
  "main": "index.js",
  "scripts": {
    "start": "npm run start_postRankings_forever && npm run start_followBackUsers_forever && npm run start_directMessageReply_forever && forever list",
    "start_directMessageReply_forever": "forever start -a -l ~/miku_twitter_bot/logs/directMessageReply_console_logs/directMessageReply_console.log ./directMessageReply.js",
    "start_followBackUsers_forever": "forever start -a -l ~/miku_twitter_bot/logs/followBackUsers_console_logs/followBackUsers_console.log ./followBackUsers.js",
    "start_postRankings_forever": "forever start -a -l ~/miku_twitter_bot/logs/postRankings_console_logs/postRankings_console.log -o ~/miku_twitter_bot/logs/postRankings_stdout_logs/postRankings_stdout.log -e ~/miku_twitter_bot/logs/postRankings_stderr_logs/postRankings_stderr.log ./postRankings.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ilPikachu/miku_twitter_bot.git"
  },
  "author": "ilPikachu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ilPikachu/miku_twitter_bot/issues"
  },
  "homepage": "https://github.com/ilPikachu/miku_twitter_bot#readme",
  "dependencies": {
    "forever": "^0.15.3",
    "jsdom": "^11.5.1",
    "moment": "^2.20.1",
    "moment-timezone": "^0.5.14",
    "node-schedule": "^1.2.5",
    "request": "^2.83.0",
    "twit": "^2.2.9"
  }
}
